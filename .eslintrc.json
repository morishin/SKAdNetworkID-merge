{
  "extends": "next/core-web-vitals",
  "overrides": [
    {
      "files": [
        "app/**/*"
      ],
      "rules": {
        "import/no-default-export": "off"
      }
    },
    {
      "files": "next.config.js",
      "rules": {
        "unused-imports/no-unused-vars": [
          "warn",
          {
            "argsIgnorePattern": "^_"
          }
        ],
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}
